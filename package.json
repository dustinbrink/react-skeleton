{
  "name": "react-skeleton",
  "version": "0.0.1",
  "description": "for testing react",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustinbrink/react-skeleton.git"
  },
  "keywords": [
    "React",
    "ES2015",
    "Webpack",
    "Babel",
    "html-webpack-plugin"
  ],
  "author": "Dustin Brink",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dustinbrink/react-skeleton/issues"
  },
  "homepage": "https://github.com/dustinbrink/react-skeleton#readme",
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "purecss": "^0.6.0",
    "purifycss-webpack-plugin": "^2.0.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0",
    "webpack-validator": "^2.2.2"
  },
  "scripts": {
    "build": "webpack",
    "clean": "rm -rf public",
    "watch": "webpack -d --watch --progress --colors",
    "start": "webpack-dev-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "stats": "webpack --profile --json > stats.json",
    "stop-win": "taskkill /f /im node.exe"
  },
  "build": {
    "src": "src",
    "app": "app",
    "dest": "public",
    "template": "index.ejs",
    "entry": "index.jsx",
    "style": "main.css"
  }
}
