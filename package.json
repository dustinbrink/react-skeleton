{
  "name": "react-skeleton",
  "version": "0.0.1",
  "description": "for testing react",
  "author": "Dustin Brink",
  "main": "src/index.jsx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustinbrink/react-skeleton.git"
  },
  "bugs": {
    "url": "https://github.com/dustinbrink/react-skeleton/issues"
  },
  "homepage": "https://github.com/dustinbrink/react-skeleton#readme",
  "dependencies": {
    "react-lite": "^0.15.14"
  },
  "keywords": [
    "boilerplate",
    "React",
    "ES2015",
    "Webpack",
    "Babel",
    "html-webpack-plugin",
    "eslint",
    "purifycss"
  ],
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-react": "^5.2.2",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "html-webpack-template": "^5.0.0",
    "postcss-loader": "^0.9.1",
    "purecss": "^0.6.0",
    "purifycss-webpack-plugin": "^2.0.3",
    "react-addons-perf": "^15.2.1",
    "style-loader": "^0.13.1",
    "stylelint": "^6.0.1",
    "stylelint-webpack-plugin": "^0.2.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0",
    "webpack-validator": "^2.2.2"
  },
  "scripts": {
    "build": "webpack",
    "clean": "rm -rf public",
    "watch": "webpack -d --watch --progress --colors",
    "start": "webpack-dev-server",
    "test": "karma start",
    "test:tdd": "npm run test -- --auto-watch --no-single-run",
    "test:lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --cache",
    "stats": "webpack --profile --json > stats.json",
    "stop:win": "taskkill /f /im node.exe"
  },
  "build": {
    "src": "src",
    "app": "app",
    "dest": "public",
    "template": "index.ejs",
    "entry": "index.jsx",
    "style": "main.css"
  }
}
